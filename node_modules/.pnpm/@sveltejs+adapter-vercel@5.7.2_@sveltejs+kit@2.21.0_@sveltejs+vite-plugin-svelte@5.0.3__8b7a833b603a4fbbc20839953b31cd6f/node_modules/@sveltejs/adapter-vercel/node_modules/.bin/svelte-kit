#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules/@sveltejs/kit/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules/@sveltejs/kit/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.0_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.30.2_vite@6.3.5_@types_563d20a9217f95bf8db90415eb03018b/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../kit/svelte-kit.js" "$@"
fi
