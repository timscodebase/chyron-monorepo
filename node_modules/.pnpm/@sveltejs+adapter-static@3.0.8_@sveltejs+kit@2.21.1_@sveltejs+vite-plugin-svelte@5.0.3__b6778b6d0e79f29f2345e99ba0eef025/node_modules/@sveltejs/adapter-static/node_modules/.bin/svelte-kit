#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules/@sveltejs/kit/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules/@sveltejs/kit/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+kit@2.21.1_@sveltejs+vite-plugin-svelte@5.0.3_svelte@5.33.1_vite@6.3.5_@types_c71512c9d556a0cc7ec4d06aaf53244f/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../kit/svelte-kit.js" "$@"
fi
