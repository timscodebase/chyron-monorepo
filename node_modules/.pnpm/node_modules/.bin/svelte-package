#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules/@sveltejs/package/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules/@sveltejs/package/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules/@sveltejs/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/@sveltejs+package@2.3.11_svelte@5.33.1_typescript@5.8.3/node_modules:/Users/tithos/Dev/Local Web/chyron-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
else
  exec node  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
fi
